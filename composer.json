{
  "name": "leadingsystems/contao-helpers",
  "description": "Leading Systems Helpers as a Contao bundle",
  "keywords": [
    "contao",
    "leading systems",
    "bundle"
  ],
  "type": "contao-bundle",
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "LeadingSystems\\HelpersBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ],
    "files": [
      "src/Resources/contao/functions.php"
    ]
  },
  "extra": {
    "contao-manager-plugin": "LeadingSystems\\HelpersBundle\\ContaoManager\\Plugin",
    "branch-alias": {
      "dev-master": "3.0.x-dev",
      "dev-develop": "3.0.x-dev"
    }
  }
}
